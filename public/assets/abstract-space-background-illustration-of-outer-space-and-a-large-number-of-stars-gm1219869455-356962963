const videoEditorPromoTile = document.getElementsByClassName("video-editor-promo-tile")[0]

const trackShowVideoEditorPromoTile = () => {
  if (window.tracking_data) 
    window.tracking_data.merchandising_units = [...(window.tracking_data.merchandising_units || []), "sub_header_video_editor_templates"]

  if (window.combinedGtmTracking != null) 
    window.combinedGtmTracking.gtmPushTracking('merchUnitInteraction', {
      'merch_unit_interaction': 'Shown',
      'merch_unit_selection': 'sub_header_video_editor_templates_shown',
      'ui_element_location': 'sub_header_merch_spot'
    })
}

const mutationObserverCallback = (mutationsList, observer) => {
  mutationsList.forEach(mutation => {
    // Once the user hovers over the Video drop down menu, the active class is added to it
      if (mutation.attributeName === 'class' && mutation.target.className === 'nav-root__list-item nav-root__list-item--delayed nav-root__list-item--active'
        && videoEditorPromoTile) 
          trackShowVideoEditorPromoTile()
  })
}

const mutationObserver = new MutationObserver(mutationObserverCallback)

if (videoMenuItem)
  mutationObserver.observe(
    videoMenuItem,
    { attributes: true }
  )

const trackClickVideoEditorPromoTile = () => {
  if (window.tracking_data) 
    window.tracking_data.merchandising_units = [...(window.tracking_data.merchandising_units || []), "sub_header_video_editor_templates"]

  if (window.combinedGtmTracking != null) 
    window.combinedGtmTracking.gtmPushTracking('merchUnitInteraction', {
      'merch_unit_interaction': 'Click',
      'merch_unit_selection': 'sub_header_video_editor_templates_clicked',
      'ui_element_location': 'sub_header_merch_spot'
    })
}

if (videoEditorPromoTile) 
  videoEditorPromoTile.addEventListener("click", trackClickVideoEditorPromoTile)</script></li><li class="nav-root__list-item" id="photos"><a class="nav-root__link" data-nav="nav=nav_Photos" href="/stock-photos">Photos</a><span class="carrot-container"><span class="carrot"></span></span><nav class="navigation-submenu navigation-submenu--hidden"><div class="navigation-submenu__photos"><button class="navigation-submenu__close-btn"></button><div class="navigation-submenu__container" id="photos"><div class="navigation-submenu__column"><div class="navigation-submenu__header">Curated content</div><div class="navigation-submenu__separator"></div><ul id="curated_content"><li><a class="navigation-submenu__link" href="/stock-photos#curated-content" id="curated_sets">Curated sets</a></li><li><a class="navigation-submenu__link" href="/collaboration/boards/AwxaA_jHWUyul-5RWiY4UA" id="signature_collection">Signature collection</a></li><li><a class="navigation-submenu__link" href="/collaboration/boards/wu0HHWcoZ0e_JoqU2nXTOg" id="essentials_collection">Essentials collection</a></li><li><a class="navigation-submenu__link" href="/diversity" id="essentials_collection">Diversity and inclusion sets</a></li></ul></div><div class="navigation-submenu__column"><div class="navigation-submenu__header">Trending searches</div><div class="navigation-submenu__separator"></div><ul id="trending_searches"><li><a class="navigation-submenu__link" href="/photos/computer">Computer</a></li><li><a class="navigation-submenu__link" href="/photos/winter-family">Winter family</a></li><li><a class="navigation-submenu__link" href="/photos/home-builder">Home builder</a></li><li><a class="navigation-submenu__link" href="/photos/christmas-ornament">Christmas ornament</a></li><li><a class="navigation-submenu__link" href="/photos/frame">Frame</a></li><li><a class="navigation-submenu__link" href="/photos/university">University</a></li></ul></div><div class="navigation-submenu__column"><div class="navigation-submenu__